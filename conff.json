{
    "compile": [
        "browserify"
        ,"conff.js"
    ]

    ,"compress": [
        "uglifyjs"
        ,{ "mangle": true, "compress": true }
    ]
    ,"watch": [
        "watchify"
        , "conff.js"
        , { "outfile": "bundle.min.js", "verbose": true }
    ]
    ,"start": [
        "conff.js compile", { "exec": true }, "|"
        ,"conff.js compress", { "pipe": true , "exec": true }
    ]
}